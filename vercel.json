{
	"version": 2,
	"builds": [
		{ "src": "server/server.js", "use": "@vercel/node" },
		{ "src": "client/dist/**", "use": "@vercel/static" }
	],
	"routes": [
		{
			"src": "/api/graphql",
			"dest": "server/server.js"
		},
		{ "src": "/(.*)", "dest": "client/dist/$1" }
	],
	"env": {
		"NODE_ENV": "development"
	}
}
